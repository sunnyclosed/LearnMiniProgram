@import "/assets/iconfont/style.wxss";

.top {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: lightgray;
  font-weight: bold;
  font-size: 32rpx;
  position: relative;
}

.top-back {
  text-align: center;
  display: block;
  width: 100rpx;
  height: 100rpx;
  font-size: 40rpx;
  position: absolute;
  left: 0;
  top: 0;
}