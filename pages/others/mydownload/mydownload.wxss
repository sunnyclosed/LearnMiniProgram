/* pages/mydownload/mydownload.wxss */
.dl {
  width: 100vw;
  height: 100vh;
}

.top {
  width: 100%;
  height: 100rpx;
  background-color: lightgray;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.top view {
  font-size: 40rpx;
  font-weight: bold;
  margin-left: 20rpx;
}

.top text {
  display: block;
  font-size: 40rpx;
  font-weight: bold;
  margin-right: 20rpx;
}

.scroll_dl {
  width: 100%;
  height: calc(100% - 100rpx);
}